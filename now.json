{
  "version": 2,
  "build": {
    "env": {
      "AUTH0_DOMAIN": "@auth0-domain",
      "AUTH0_CLIENT_ID": "@auth0-client-id",
      "AUTH0_CLIENT_SECRET": "@auth0-client-secret",
      "REDIRECT_URI": "https://records-rho.vercel.app/api/callback",
      "POST_LOGOUT_REDIRECT_URI": "https://records-rho.vercel.app/",
      "SESSION_COOKIE_SECRET": "@session-cookie-secret",
      "HASURA_GRAPHQL_URL": "@hasura-graphql-url",
      "HASURA_GRAPHQL_URL_WSS": "@hasura-graphql-url-wss",
      "SECURE_COOKIE": "true"
    }
  }
}